@typeparam D
@using BasicBlazorLibrary.Components.RenderHelpers
@if (DeckObject == null)
{
    return; //i think if setting as nothing, go ahead and dispose everything afterall.
}
@if (UseKey == false)
{
    @ChildContent
}
else
{
    <RefreshComponent CanRender="@CanRender"
                  Key="DeckObject.GetKey()">
        @ChildContent
    </RefreshComponent>
}