    <MainTemplate Context="Card">
        <CardGraphicsBlazor ClickBlank="true" @key="Card.Deck" TargetHeight="@RealHeight" DeckObject="Card" CommandParameter="Card" CommandObject="DeckPile!.DeckObjectClickedCommand" />
    </MainTemplate>
</BaseDrawPileBlazor>