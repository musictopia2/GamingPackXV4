@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<CascadingValue Value="@TargetImageHeight">
    <ParentControl VM="TileRummyMainViewModel">
        <TileRummyMainView />
    </ParentControl>
    <ParentControl VM="MultiplayerOpeningViewModel<TileRummyPlayerItem>">
        <MultiplayerOpeningView P="TileRummyPlayerItem" />
    </ParentControl>
</CascadingValue>