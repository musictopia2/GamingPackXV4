@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<ParentControl VM="TroubleMainViewModel">
    <TroubleMainView />
</ParentControl>
<ParentControl VM="MultiplayerOpeningViewModel<TroublePlayerItem>">
    <MultiplayerOpeningView P="TroublePlayerItem" />
</ParentControl>
<ParentControl VM="BeginningChooseColorViewModel<EnumColorChoice, TroublePlayerItem>">

    <BeginningChooseColorView E="EnumColorChoice" Context="Piece" P="TroublePlayerItem">
        <MarblePiece MainColor="@GetColor(Piece.EnumValue)" />
    </BeginningChooseColorView>
</ParentControl>