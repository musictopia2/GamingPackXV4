@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<CascadingValue Value="TargetHeight">
    <ParentControl VM="Pinochle2PlayerMainViewModel">
        <Pinochle2PlayerMainView />
    </ParentControl>
    <ParentControl VM="MultiplayerOpeningViewModel<Pinochle2PlayerPlayerItem>">
        <MultiplayerOpeningView P="Pinochle2PlayerPlayerItem" />
    </ParentControl>
</CascadingValue>