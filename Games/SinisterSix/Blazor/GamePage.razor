@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<ParentControl VM="SinisterSixMainViewModel">
    <SinisterSixMainView />
</ParentControl>
<ParentControl VM="MultiplayerOpeningViewModel<SinisterSixPlayerItem>">
    <MultiplayerOpeningView P="SinisterSixPlayerItem" />
</ParentControl>