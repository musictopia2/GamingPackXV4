@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<CascadingValue Value="TargetHeight">
    <ParentControl VM="DominosRegularMainViewModel">
        <DominosRegularMainView />
    </ParentControl>
    <ParentControl VM="MultiplayerOpeningViewModel<DominosRegularPlayerItem>">
        <MultiplayerOpeningView P="DominosRegularPlayerItem" />
    </ParentControl>
</CascadingValue>