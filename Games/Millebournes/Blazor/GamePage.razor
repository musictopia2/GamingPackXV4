@if (GameData == null || TestData == null || BasicData == null)
{
    if (GameData == null)
    {
        <div>No Game Data</div>
    }
    if (TestData == null)
    {
        <div>No Test Data</div>
    }
    if (BasicData == null)
    {
        <div>No Basic Data</div>
    }
    return;
}
<CascadingValue Value="TargetHeight">
    <ParentControl VM="MillebournesMainViewModel">
        <MillebournesMainView />
    </ParentControl>
    <ParentControl VM="MultiplayerOpeningViewModel<MillebournesPlayerItem>">
        <MultiplayerOpeningView P="MillebournesPlayerItem" />
    </ParentControl>
</CascadingValue>