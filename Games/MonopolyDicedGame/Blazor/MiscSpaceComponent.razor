<SimpleRepeater HowMany="3" Context="item">
    @{
        var vector = GetSpaceInfo(item);
        var category = GetType(item);
    }
    <SpaceBlazor MarginLeft="4" MarginTop="5" Row="vector.Row" Column="vector.Column">
        @if (category == EnumMiscType.None)
        {
            <ColorSpaceComponent Color="@GetActionColor" TargetImageHeight="@TargetImageHeight" />
            return;
        }
        <MiscDiceBlazor TargetHeight="@TargetImageHeight" Category="@category" />
    </SpaceBlazor>
</SimpleRepeater>