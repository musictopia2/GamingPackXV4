@inherits GraphicsCommand
<svg @onclick="Submit">
    <ResourceImage Width="100%" Assembly="GetAssembly" Height="100%" FileName="leaf.png" />
    @if (Space!.HasFrog)
    {
        <ResourceImage Width="100%" Assembly="GetAssembly" Height="100%" FileName="frog.png" />
    }
    @if (Space.IsSelected || Space.IsTarget)
    {
        <rect width="100%" fill="none" height="100%" style="@GetStrokeInfo()" />
    }
</svg>